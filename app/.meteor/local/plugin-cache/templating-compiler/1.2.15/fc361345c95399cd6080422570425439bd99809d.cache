{"head":"","body":"","js":"\nTemplate.__checkName(\"layout_header\");\nTemplate[\"layout_header\"] = new Template(\"Template.layout_header\", (function() {\n  var view = this;\n  return HTML.HEADER({\n    class: function() {\n      return [ \"search-area \", Spacebars.mustache(view.lookup(\"disabled\")) ];\n    },\n    style: function() {\n      return [ \"background-image:\", Spacebars.mustache(view.lookup(\"backgroundDataUrl\")) ];\n    }\n  }, \"\\n\\t\\t\", HTML.DIV(\"\\n        \", HTML.Raw(\"<h1>Look up an Ethereum Name</h1>\"), \"\\n\\t    \", HTML.INPUT({\n    id: \"search-input\",\n    class: \"search-input\",\n    type: \"text\",\n    placeholder: \"type a name\",\n    value: function() {\n      return Spacebars.mustache(view.lookup(\"returnedName\"));\n    }\n  }), \"\\n        \", HTML.Raw('<a href=\"#\" class=\"tld\">.eth</a>'), \"\\n\\t    \\n        \", Blaze.View(\"lookup:searched\", function() {\n    return Spacebars.mustache(view.lookup(\"searched\"));\n  }), \"\\n        \\n\\t\\t\"), \"\\n\\t\");\n}));\n","bodyAttrs":{}}